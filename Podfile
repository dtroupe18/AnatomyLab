# Uncomment the next line to define a global platform for your project
platform :ios, ’10.2’

target 'CadaverLog' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CadaverLog
  pod 'AWSCore'
  pod 'AWSAutoScaling'
  pod 'AWSCloudWatch'
  pod 'AWSDynamoDB'
  pod 'AWSEC2'
  pod 'AWSElasticLoadBalancing'
  pod 'AWSKinesis'
  pod 'AWSLambda'
  pod 'AWSMachineLearning'
  pod 'AWSMobileAnalytics'
  pod 'AWSS3'
  pod 'AWSSES'
  pod 'AWSSimpleDB'
  pod 'AWSSNS'
  pod 'AWSSQS'
  pod 'AWSCognito'
  pod 'GSMessages'
  pod 'IQKeyboardManagerSwift'

  # install all AWS Pods

  target 'CadaverLogTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CadaverLogUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
